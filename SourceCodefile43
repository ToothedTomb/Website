<h1><u>Sorce code file for Temperature Converter!</u></h1>
<p>#!/usr/bin/env python3 <br>
import tkinter as tk <br>
window = tk.Tk() <br>
window.title("Temperature Converter by Jonathan Steadman") <br>
frm_entry = tk.Frame(master=window) <br>
ent_temperature = tk.Entry(master=frm_entry, width=10) <br>
lbl_temp = tk.Label(master =frm_entry, text="\N{DEGREE FAHRENHEIT}") <br>
ent_temperature.grid(row=0, column=0, sticky="e") <br>
lbl_temp.grid(row=0, column=1, sticky="w") <br>
def fahrenheit_to_celsius(): <br>
    fahrenheit = ent_temperature.get() <br>
    celsius = (5/9) * (float(fahrenheit) - 32) <br>
    lbl_result["text"] = f"{round(celsius, 2)} \N{DEGREE CELSIUS}" <br>

btn_convert = tk.Button( <br>
    master=window, <br>
    text="\N{RIGHTWARDS BLACK ARROW}", <br>
    command=fahrenheit_to_celsius <br>
)
lbl_result = tk.Label(master=window, text="\N{DEGREE CELSIUS}") <br>
frm_entry.grid(row=0, column=0, padx=10) <br>
btn_convert.grid(row=0, column=1, padx=10) <br>
lbl_result.grid(row=0, column=2, padx=10) <br>
window.mainloop() <br>
</p>
